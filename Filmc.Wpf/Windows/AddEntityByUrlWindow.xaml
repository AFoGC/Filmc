<Window x:Class="Filmc.Wpf.Windows.AddEntityByUrlWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Filmc.Wpf.Windows"
        mc:Ignorable="d"
        Title="AddEntityByUrlWindow" Height="450" Width="800">
    <Grid>
        <StackPanel Width="400">
            <TextBox Height="20" Name="UrlText" Text=""/>
            <Button Content="Add" Click="AddEntity"/>
        </StackPanel>
        <Button HorizontalAlignment="Right" VerticalAlignment="Top"
                Width="30" Height="30" Click="CloseWindow"/>
    </Grid>
</Window>
